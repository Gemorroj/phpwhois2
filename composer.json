{
    "name": "gemorroj/phpwhois2",
    "description": "PHPWhois2 - library for querying whois services and parsing results.",
    "keywords": [
        "PHP",
        "Whois",
        "phpWhois"
    ],
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Kevin Lucich",
            "email": "info@lucichkevin.it"
        },
        {
            "name": "Mark Jeftovic",
            "email": "markjr@easydns.com"
        },
        {
            "name": "David Saez Padros",
            "email": "david@ols.es"
        },
